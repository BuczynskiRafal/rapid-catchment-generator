{
  "version": "1.0",
  "description": "Fuzzy logic rules for catchment parameter calculation",
  "rules": [
    {
      "name": "mountains_vegetated_on_marshes",
      "when": {
        "land_cover": "mountains_vegetated",
        "land_form": "marshes_and_lowlands"
      },
      "then": {
        "slope": "flats_and_plateaus",
        "impervious": "mountains_vegetated",
        "catchment": "meadows"
      }
    },
    {
      "name": "mountains_rocky_on_marshes",
      "when": {
        "land_cover": "mountains_rocky",
        "land_form": "marshes_and_lowlands"
      },
      "then": {
        "slope": "flats_and_plateaus",
        "impervious": "mountains_vegetated",
        "catchment": "meadows"
      }
    },
    {
